{
  "name": "mattfalahe/manager-core",
  "description": "Core management plugin for SeAT - Provides appraisal services, market pricing, and inter-plugin bridge functionality",
  "type": "seat-plugin",
  "keywords": [
    "seat",
    "eve",
    "eveonline",
    "manager",
    "appraisal",
    "pricing",
    "bridge",
    "plugin",
    "core"
  ],
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Matt Falahe",
      "email": "mattfalahe@gmail.com"
    }
  ],
  "homepage": "https://github.com/MattFalahe/Manager-Core",
  "require": {
    "php": "^8.0",
    "laravel/framework": "^10.0",
    "eveseat/web": "^5.0",
    "eveseat/services": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "orchestra/testbench": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "ManagerCore\\": "src/",
      "ManagerCore\\Database\\Seeders\\": "src/Database/seeders"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ManagerCore\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ManagerCore\\ManagerCoreServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
